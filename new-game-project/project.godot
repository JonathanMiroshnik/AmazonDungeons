; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="AWSChallenge"
run/main_scene="res://scenes/main_menu/menu.tscn"
config/features=PackedStringArray("4.3", "C#", "Forward Plus")
config/icon="res://textures/icon.svg"

[autoload]

GameManager="*res://GameManager.cs"

[dotnet]

project/assembly_name="New Game Project"

[editor_plugins]

enabled=PackedStringArray("res://addons/gd-blender-3d-shortcuts/plugin.cfg")

[physics]

3d/physics_engine="JoltPhysics3D"

[rendering]

global_illumination/gi/use_half_resolution=true
global_illumination/sdfgi/probe_ray_count=0
global_illumination/sdfgi/frames_to_converge=2
global_illumination/sdfgi/frames_to_update_lights=4
